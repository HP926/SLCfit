<div class="container-fluid">
  <div class="row">
    <div class="col-sm-2">
      <div class="row side-nav">
        <%= render(partial: 'shared/side_nav') %>
      </div>
    </div>
    <div class="col-sm-10">
      <h2 class="page-header">My Goals</h2>
      <h3>stuff</h3>
      <% @goals.each do |goal|  %>

        <h3>you want to <%= link_to goal.description, goal_weeks_path(goal.id) %></h3>
        <p><%= goal.reason%></p>
        <p>you have <%= goal.time_frame %> weeks to complete this goal.</p>
      <%= link_to 'Edit', edit_goal_path(goal.id) %> |
      <%= link_to 'Delete', goal_path(goal.id), method: :delete, data: {confirm: "Are you sure?"}%>
      <% end %>
    <%= link_to 'New Goal', new_goal_path %>
      </div>

