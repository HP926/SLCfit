<div class="container">
  <div class="row">
    <div class="col-md-3">
      <div class="row side-nav">
        <%= render(partial: 'shared/side_nav') %>
      </div>
    </div>
    <div class="col-md-4">
      <div><%= image_tag current_user.image(:thumb) %>
      <%= current_user.username %>
      <% @goals.each do |goal|  %>

        <h3><%= link_to goal.description, goal_weeks_path(goal.id) %></h3>
        <p><%= goal.reason%></p>
        <p><%= goal.time_frame %></p>
      <%= link_to 'Edit', edit_goal_path(goal.id) %> |
      <%= link_to 'Delete', goal_path(goal.id), method: :delete, data: {confirm: "Are you sure?"}%>
      <% end %>
      </div>
    </div>
