<h3 class=""><%= "Week #{index + 1}" %></h3>
<div class="col-sm-12">
  <div id="progress-<%= week.id %>">
    <%= render partial: 'activities/progress', locals: {week: week} %>
  </div>

  <div id="calendar-<%= week.id %>">
    <%= render partial: '/activities/calendar', locals: {week: week} %>
  </div>

  <div class="bank-<%= week.id %>">
    <%= render partial: 'activities/bank', locals: {week: week} %>
  </div>
</div>
